{
  "name": "poe-js-sdk",
  "version": "2.0.10",
  "type": "module",
  "description": "TypeScript SDK for the Path of Exile API",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./browser-auth": {
      "types": "./dist/browser-auth.d.ts",
      "import": "./dist/browser-auth.mjs",
      "require": "./dist/browser-auth.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "prepare": "npm run build",
    "test": "jest --coverage",
    "test:examples": "tsx examples/run-tests.ts",
    "test:leagues": "tsx examples/leagues-test.ts",
    "test:comprehensive": "tsx examples/comprehensive-test.ts",
    "lint": "eslint src --ext .ts --fix",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "docs": "npm --prefix website run start",
    "docs:build": "npm --prefix website run build",
    "docs:serve": "npm --prefix website run serve",
    "docs:deploy": "npm --prefix website run deploy",
    "release:patch": "npm version patch -m \"chore(release): %s\" && git push --follow-tags",
    "release:minor": "npm version minor -m \"chore(release): %s\" && git push --follow-tags",
    "release:major": "npm version major -m \"chore(release): %s\" && git push --follow-tags"
  },
  "keywords": [
    "path-of-exile",
    "poe",
    "api",
    "sdk",
    "typescript",
    "gaming"
  ],
  "author": "Vito Pistelli",
  "license": "MIT",
  "engines": {
    "node": ">=20.19.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viligante8/poe-js-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/viligante8/poe-js-sdk/issues"
  },
  "homepage": "https://github.com/viligante8/poe-js-sdk#readme",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unicorn": "^61.0.2",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.6",
    "vite-plugin-dts": "^4.5.4"
  },
  "dependencies": {
    "axios": "^1.12.2"
  }
}
